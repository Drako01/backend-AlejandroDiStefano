<link rel="stylesheet" href="//cdn.jsdelivr.net/npm/sweetalert2@11.1.3/dist/sweetalert2.min.css">
<section class="col d-flex flex-column justify-content-center align-items-center michat">
    <div class="container card-chat">
        <div class="col-sm-6 col-sm-offset-3 form-box ">
            {{#if user}}
            <h3 id="UserNameChat">Bienvenido {{user.first_name}} {{user.last_name}} {{user.user.first_name }} al Chat de
                Lonne Open</h3>
            <p>
                EMail del Usuario:
            </p>
            <p id="username">
                {{ user.email }} {{ user.user.email }}
            </p>
            {{/if}}
            <input type="text" id="chatBox" class="form-control" placeholder="Escribe un mensaje">
            <hr>
            <h3>Historial de Mensajes</h3>
            <div id="history"></div>

        </div>
    </div>
</section>


<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11.1.3/dist/sweetalert2.min.js"></script>

<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
<script src="/socket.io/socket.io.js"></script>
<script src="/index.js"></script>
<script src="/email.js"></script>
<script src="/chat.js"></script>